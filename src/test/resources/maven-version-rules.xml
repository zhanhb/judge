<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://www.mojohaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <ignoreVersions>
        <ignoreVersion type="regex">(?i).*[-\.](?:alpha|beta|cr|ea|edr|pre?|rc)(?:-?\d*(?:\.\d+)*)?(?:-\d)?</ignoreVersion>
        <ignoreVersion type="regex">(?i).*[\d\.-](?:b|M)0?\d*</ignoreVersion>
    </ignoreVersions>

    <rules>
        <rule groupId="antlr" artifactId="antlr">
            <ignoreVersions>
                <ignoreVersion>20030911</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="com.microsoft.sqlserver" artifactId="mssql-jdbc">
            <ignoreVersions>
                <ignoreVersion type="regex">.+-preview</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="commons-collections" artifactId="commons-collections">
            <ignoreVersions>
                <ignoreVersion>20030418.083655</ignoreVersion>
                <ignoreVersion>20031027.000000</ignoreVersion>
                <ignoreVersion>20040102.233541</ignoreVersion>
                <ignoreVersion>20040616</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="com.google.guava" artifactId="guava">
            <ignoreVersions>
                <ignoreVersion type="regex">.+-android</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="dom4j" artifactId="dom4j">
            <ignoreVersions>
                <ignoreVersion>20040902.021138</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="javax.cache" artifactId="cache-api">
            <ignoreVersions>
                <ignoreVersion type="regex">.+-PFD</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="javax.enterprise" artifactId="cdi-api">
            <ignoreVersions>
                <ignoreVersion type="regex">.+Alpha\d[a-z]</ignoreVersion>
                <ignoreVersion type="regex">.+-[A-Z]{3}(?:\d+(?:\.\d+)*)?</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="javax.faces" artifactId="javax.faces-api">
            <ignoreVersions>
                <ignoreVersion type="regex">.+-pfd</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="javax.xml.bind" artifactId="jaxb-api">
            <ignoreVersions>
                <ignoreVersion type="regex">.+-b\d+\.\d+</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.postgresql" artifactId="postgresql">
            <ignoreVersions>
                <ignoreVersion type="regex">.+\.jre[67]</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.springframework.data" artifactId="spring-data-releasetrain">
            <ignoreVersions>
                <ignoreVersion>1.3.0.RELEASE</ignoreVersion>
                <ignoreVersion>1.4.6.RELEASE</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.webjars" artifactId="ace">
            <ignoreVersions>
                <ignoreVersion>07.31.2013</ignoreVersion>
                <ignoreVersion>04.09.2013</ignoreVersion>
                <ignoreVersion>01.08.2014</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="xml-apis" artifactId="xml-apis">
            <ignoreVersions>
                <ignoreVersion>2.0.0</ignoreVersion>
                <ignoreVersion>2.0.2</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
</ruleset>
